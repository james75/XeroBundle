{
    "name": "blackoptic/xerobundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Bundle that creates an Guzzle client for Xero",
    "keywords": ["symfony2", "xero", "accounts", "guzzle"],
    "homepage": "https://github.com/james75/XeroBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "James Watson"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-openssl": "*",
        "symfony/symfony": "2.*",
        "guzzle/guzzle": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "BlackOptic\\Bundle\\XeroBundle": ""
        }
    },
    "target-dir": "BlackOptic/Bundle/XeroBundle"
}
